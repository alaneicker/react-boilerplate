{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "targets": {
        "ie": "11"
      },
      "corejs": 3
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-transform-runtime", { "corejs": 3 }]
  ],
  "env": {
    "node": {
      "plugins": [
        ["babel-plugin-transform-remove-imports", { "test": "(.css|.md|.json)$" }]
      ]
    }
  }
}
