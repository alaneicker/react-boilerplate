{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React Boilerplate",
  "scripts": {
    "start": "NODE_ENV=production node dist/server/index.js",
    "start:dev": "webpack-dev-server --mode=development --env.NODE_ENV=development --content-base public/ ---port 4000 --open",
    "build": "webpack --mode=production --env.NODE_ENV=production && npm run build:server && npm run build:src && mv dist/index.html dist/template.html",
    "build:server": "babel -d ./dist/server ./server -s",
    "build:src": "BABEL_ENV=node babel -d ./dist/src ./src -s",
    "test": "jest --watchAll --coverage --config=./jestConfigs/jest.config.js",
    "lint": "eslint 'src/**/*.js' --ignore-pattern '*.test.js'"
  },
  "repository": "https://github.com/alaneicker/react-boilerplate",
  "author": "Alan Eicker",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "highlight.js": "^10.4.1",
    "path": "^0.12.7",
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-remove-imports": "^1.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.2.0",
    "terser-webpack-plugin": "^1.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
